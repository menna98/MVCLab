@model List<Car>;

@{
    var cars = Model;
    void displayImg(Car c)
    {
        <figure>
            <img src="data:image/jpeg;base64,"
             alt="@c.Name">
           @* alt="@($"{c.Model} from {c.Manufacturer}")">
            <figcaption>@($"{c.Model} from {c.Manufacturer}")</figcaption> *@
            <figcaption>@c.Name</figcaption>
        </figure>
    }
}

<ul>
    @foreach(var car in cars)
    {
        <li>
            <ul>
                //<li>@($"Name: {car.Model} from {car.Manufacturer}")</li>
                <li>@car.Name</li>
                <li> @($"usage Duration: {car.Duration} Year")</li>
                <li>@Html.Raw(@car.HtmlDescription)</li>   
                <li>@{displayImg(car);}</li>
            </ul>
        </li>
    }
</ul>


@{
    void RenderTableBody()
    {
        <tr>
            <th>#</th>
            <th>Name</th>
            <th>usageDuration</th>
            <th>HtmlDescription</th>
            <th>img</th>
        </tr>
        @for (int i = 0; i < cars.Count; i++)
        {
            <tr>
                <td>@i</td>
                <td>@($"{cars[i].Name}")</td>
               @* <td>@($"{cars[i].Model} from {cars[i].Manufacturer}")</td> *@
                <td>@($"{cars[i].Duration} Year")</td>
                @* <td>@($"{DateTime.Now.Year - cars[i].FirstUseDate.Year} Year")</td> *@
                <td>@Html.Raw(@cars[i].HtmlDescription)</td>
                <td>@{displayImg(cars[i]);}</td>
            </tr>
        }
    }
}

<table>
    @{
        RenderTableBody();
    }
</table>